{
  "name": "sudoku-ps",
  "version": "1.0.0",
  "description": "Sudoko game / solver in Purescript/Halogen",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bulma": "^0.8.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "purescript": "^0.13.5",
    "spago": "^0.13.1"
  },
  "scripts": {
    "test": "spago test",
    "build": "spago build",
    "clean": "rm -rf node_modules output .spago dist/*",
    "bundle": "spago bundle-app --main Main --to dist/app.js && parcel build",
    "start": "spago build && parcel index.html"
  },
  "keywords": [
    "purescript",
    "halogen",
    "sudoku"
  ],
  "author": "Carsten Koenig",
  "license": "ISC"
}